{
  "name": "livghit/laragrounds",
  "description": "Composer Package featuring exquisitely crafted background snippets by @ibelick.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel-backgrounds"
  ],
  "version": "1.x",
  "autoload": {
    "psr-4": {
      "Laragrounds\\": "src/"
    }
  },
  "authors": [
    {
      "name": "livghit"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laragrounds\\LaragroundsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
